alias caddysetcap="sudo setcap cap_net_bind_service=+ep $(which caddy)"
alias caddystatus="sudo service caddy status"
alias caddystart="sudo service caddy start"
alias caddystop="sudo service caddy stop"
alias maj="sudo apt-get update"
alias Maj="sudo apt-get upgrade"
alias db="cd /home/www/sites/voiretmanger/ && rm *.sql &&  wp db export"
alias files="find . -type f | wc -l"
alias majwpcli="cd /home/www/sites/voiretmanger/ && sudo wp cli update --allow-root"
alias logs="tail -f /home/www/logs/voiretmanger/access.log | grep 'HTTP/2.0'"
alias caddymaj="curl https://getcaddy.com | bash -s personal && caddysetcap && caddyreload"
alias tail="sudo tail -f"
alias caddyreload="sudo service caddy reload"